    "./dev/dashboard/chart-component-4.component.ts",
    "./dev/dashboard/chart-component-5.component.ts",
    "./dev/dashboard/chart-component-6.component.ts",
    "./dev/dashboard/chart-component-7.component.ts",
    "./dev/dashboard/chart-component-8.component.ts",
